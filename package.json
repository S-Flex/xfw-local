{
  "name": "xfw-local",
  "version": "1.0.0",
  "description": "allows xfw-portal to perform local actions",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-all": "electron-packager . xfw-local --all --icon=./icon/sflex_logo.icns",
    "build-macos": "electron-packager . xfw-local --platform=darwin --arch=all --icon=./icon/sflex_logo.icns",
    "build-linux": "electron-packager . xfw-local --platform=linux --arch=all --icon=./icon/sflex_logo.icns",
    "build-windows": "electron-packager . xfw-local --platform=win32 --arch=all --icon=./icon/sflex_logo.icns"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/S-Flex/xfw-local.git"
  },
  "author": "Alex Spelt",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/S-Flex/xfw-local/issues"
  },
  "homepage": "https://github.com/S-Flex/xfw-local#readme",
  "devDependencies": {
    "electron-packager": "^17.1.2",
    "electron-reloader": "^1.2.3",
    "electron": "^26.1.0"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
